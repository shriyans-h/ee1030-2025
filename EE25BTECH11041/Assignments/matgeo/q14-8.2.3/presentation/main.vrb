\frametitle{Python code with shared object}
\begin{lstlisting}
# Plot parabola
plt.figure(figsize=(6,6))
plt.scatter(x, y, s=5, c='b', label=r"$y^2=-8x$")
plt.axhline(0, color='k', linewidth=0.8)
plt.axvline(0, color='k', linewidth=0.8)
plt.xlabel("x-axis")
plt.ylabel("y-axis")
plt.title("Parabola: $y^2 = -8x$")
plt.legend()
plt.grid(True)
plt.axis("equal")
plt.show()

\end{lstlisting}
